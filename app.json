{
  "expo": {
    "name": "weatherApp",
    "slug": "weatherApp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Create a weather application using react-native. This app shows the weather based on the user's location with the brief description. ",
    "githubUrl": "https://github.com/pachopa/weather-app"
  }
}
